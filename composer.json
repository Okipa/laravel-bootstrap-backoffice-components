{
    "name": "okipa/laravel-bootstrap-components",
    "description": "Ready-to-use and customizable components.",
    "keywords": [
        "okipa",
        "package",
        "laravel",
        "bootstrap",
        "php",
        "components",
        "generate",
        "generator",
        "generation",
        "html"
    ],
    "homepage": "https://github.com/Okipa/laravel-bootstrap-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4||^8.0",
        "illuminate/support": "^7.0||^8.0",
        "okipa/laravel-html-helper": "dev-php8-support"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "nunomaduro/larastan": "^0.6",
        "orchestra/testbench": "^5.0||^6.0",
        "phpmd/phpmd": "^2.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelBootstrapComponents\\": "src/"
        },
        "files": [
            "src/Helpers/components.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelBootstrapComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd config,src text phpmd.xml",
            "vendor/bin/phpstan analyse --error-format=table --memory-limit=2048M",
            "vendor/bin/phpunit -d memory_limit=-1 --testdox"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelBootstrapComponents\\ComponentServiceProvider"
            ],
            "aliases": {
                "InputText": "Okipa\\LaravelBootstrapComponents\\Facades\\InputText",
                "InputEmail": "Okipa\\LaravelBootstrapComponents\\Facades\\InputEmail",
                "InputPassword": "Okipa\\LaravelBootstrapComponents\\Facades\\InputPassword",
                "InputUrl": "Okipa\\LaravelBootstrapComponents\\Facades\\InputUrl",
                "InputTel": "Okipa\\LaravelBootstrapComponents\\Facades\\InputTel",
                "InputNumber": "Okipa\\LaravelBootstrapComponents\\Facades\\InputNumber",
                "InputColor": "Okipa\\LaravelBootstrapComponents\\Facades\\InputColor",
                "InputDate": "Okipa\\LaravelBootstrapComponents\\Facades\\InputDate",
                "InputTime": "Okipa\\LaravelBootstrapComponents\\Facades\\InputTime",
                "InputDatetime": "Okipa\\LaravelBootstrapComponents\\Facades\\InputDatetime",
                "InputFile": "Okipa\\LaravelBootstrapComponents\\Facades\\InputFile",
                "InputCheckbox": "Okipa\\LaravelBootstrapComponents\\Facades\\InputCheckbox",
                "InputToggle": "Okipa\\LaravelBootstrapComponents\\Facades\\InputToggle",
                "InputRadio": "Okipa\\LaravelBootstrapComponents\\Facades\\InputRadio",
                "Textarea": "Okipa\\LaravelBootstrapComponents\\Facades\\Textarea",
                "Select": "Okipa\\LaravelBootstrapComponents\\Facades\\Select",
                "Submit": "Okipa\\LaravelBootstrapComponents\\Facades\\Submit",
                "SubmitValidate": "Okipa\\LaravelBootstrapComponents\\Facades\\SubmitValidate",
                "SubmitCreate": "Okipa\\LaravelBootstrapComponents\\Facades\\SubmitCreate",
                "SubmitUpdate": "Okipa\\LaravelBootstrapComponents\\Facades\\SubmitUpdate",
                "Button": "Okipa\\LaravelBootstrapComponents\\Facades\\Button",
                "ButtonLink": "Okipa\\LaravelBootstrapComponents\\Facades\\ButtonLink",
                "ButtonBack": "Okipa\\LaravelBootstrapComponents\\Facades\\ButtonBack",
                "ButtonCancel": "Okipa\\LaravelBootstrapComponents\\Facades\\ButtonCancel",
                "Image": "Okipa\\LaravelBootstrapComponents\\Facades\\Image",
                "Audio": "Okipa\\LaravelBootstrapComponents\\Facades\\Audio",
                "Video": "Okipa\\LaravelBootstrapComponents\\Facades\\Video"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
